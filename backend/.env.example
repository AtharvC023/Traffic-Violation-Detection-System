# Environment Configuration for Traffic Violation Detection System

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/traffic_violations
REDIS_URL=redis://localhost:6379/0

# AI API Keys
OPENAI_API_KEY=your-openai-api-key-here
LLAMA_API_KEY=your-groq-api-key-here
LLAMA_API_URL=https://api.groq.com/openai/v1

# JWT Configuration
SECRET_KEY=your-super-secret-jwt-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# File Upload Settings
MAX_UPLOAD_SIZE=10485760  # 10MB
UPLOAD_FOLDER=./uploads

# Camera Settings
DEFAULT_CAMERA_TIMEOUT=30
MAX_CONCURRENT_STREAMS=10

# Background Task Settings
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/app.log

# Rate Limiting
RATE_LIMIT_PER_MINUTE=100

# Development Settings
DEBUG=True
ENVIRONMENT=development